/** 
 * Multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=nsel);
    And(a=a, b=nsel, out=asel);
    And(a=b, b=sel, out=bsel);
    Or(a=asel, b=bsel, out=out);
}
